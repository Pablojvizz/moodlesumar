/* Forum */
.forumpost .topic {
  background: #eee url([[pix:theme|current_bg]]) repeat-x 0 0;
  border: 1px solid #ddd;
  -webkit-border-radius:6px;
  -moz-border-radius:6px;
  border-radius:6px;
}
.forumpost .topic div, .forumpost .content div {
  margin:4px;
}
.forumpost .starter .subject {
  font-weight:bold;
  color: #DD7626;
  font-size: 1.2em;
}
