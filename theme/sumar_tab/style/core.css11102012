}

.hidepanelicon , .hidepanelicon img{
width: 18px !important;
height: 20px !important;
display: inline-block !important;
}

.smallicon{
top:-1px !important}

/* Calendario Empieza */ 

.block .minicalendar {
  border-collapse: separate;
  border-spacing: 3px;
}

.block .calendar-controls .current a:link, a:visited {
	color:#000;
  text-decoration: none;
  text-transform: capitalize;

}

.block .minicalendar .weekdays th {
border-color:#ECEFF6;
}

.block .minicalendar td.weekend {
color: #5D849E;
}
.block .minicalendar td, .block .minicalendar th {
  border: 0;
  background: transparent;
}


.block .minicalendar td {
  border: 0px solid #CACACA;
  background: transparent;
}

.block .minicalendar td.today {

  border-color:#CACACA;
  color:#183A65;
  background-color: #183A65;

}
.minicalendar td.today a:link, a:visited .block  {
  color:#FFF !important;
  text-decoration: none;
  }
li.current_branch{
color: #183A65 !important}

  .block .calendar-controls .previous  {  
  color:#606060;
  }
    .block .calendar-controls .next  {  
  color:#606060;
  }
  
 /* Calendario termina */  

