/* Content */
.headingblock {
background:url([[pix:theme|vector]]) no-repeat 10px center;
  background-color:#66A7BB;
  color:#fff;
  font-weight:700;
  border:none;
  padding: 4px 38px;
  -webkit-border-radius:14px;
  -moz-border-radius:14px;
  border-radius:14px;
