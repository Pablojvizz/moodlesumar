  border: 1px solid #ddd;
  background:#fff;
  -webkit-border-radius:6px;
  -moz-border-radius:6px;
  border-radius:6px;
}
/* Footer */
#page-footer {
    clear: both;
	border-top: 0px dotted #DDD;
	background: #66A7BB url([[pix:theme|footer_bkg]]) repeat-x top;
	font-size: 11px;
	min-height: 15px;
	padding: 0 12px 12px;
	overflow: hidden;
}
#page-footer .credits {
	float: right;
	margin-right: 15px;
	width:109px;
	height:31px;
	padding: 12px 4px 12px 12px;
	background: #66A7BB url([[pix:theme|foot_]]) no-repeat;
	
}
#page-footer .footermenu {
	float: left;
	height: auto;
	width: auto;
	margin-top:15px;
	padding: 0;
}
.footermenu .logininfo {
	font-size: 11px;
	white-space: nowrap;
	background: transparent url([[pix:theme|logininfo_bg]]) no-repeat 0 5px;
	padding: 2px 0 2px 25px !important;
	color: #fff !important;
	text-align:left;
}
#page-footer a:link,#page-footer a:visited{
color:#cbde91 !important;
border: none !important;
}
/* Dock */
