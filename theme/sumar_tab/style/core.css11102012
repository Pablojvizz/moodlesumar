/*set*/
@font-face {
    font-family: 'DroidSansRegular';
    src: url('http://fonts.syncx.com.ar/droid_sans/DroidSans-webfont.eot');
    src: url('http://fonts.syncx.com.ar/droid_sans/DroidSans-webfont.eot?#iefix') format('embedded-opentype'),
         url('http://fonts.syncx.com.ar/droid_sans/DroidSans-webfont.woff') format('woff'),
         url('http://fonts.syncx.com.ar/droid_sans/DroidSans-webfont.ttf') format('truetype'),
         url('http://fonts.syncx.com.ar/droid_sans/DroidSans-webfont.svg#DroidSansRegular') format('svg');
    font-weight: normal;
    font-style: normal;

}

@font-face {
    font-family: 'DroidSansBold';
    src: url('http://fonts.syncx.com.ar/droid_sans/DroidSans-Bold-webfont.eot');
    src: url('http://fonts.syncx.com.ar/droid_sans/DroidSans-Bold-webfont.eot?#iefix') format('embedded-opentype'),
         url('http://fonts.syncx.com.ar/droid_sans/DroidSans-Bold-webfont.woff') format('woff'),
         url('http://fonts.syncx.com.ar/droid_sans/DroidSans-Bold-webfont.ttf') format('truetype'),
         url('http://fonts.syncx.com.ar/droid_sans/DroidSans-Bold-webfont.svg#DroidSansBold') format('svg');
    font-weight: normal;
    font-style: normal;

}
.style1 {
    font-family: 'DroidSansRegular', Arial, sans-serif;
}                                    
.style2 {
    font-family: 'DroidSansBold', Arial, sans-serif;
}
*{ outline:none}
html{
	color:#183A65 !important;}
body {
	line-height: 1.4em;
	font-size: 12px;
	color: #183A65;
	font-family: 'DroidSansRegular', Arial, sans-serif;
	background: #FFF;
	margin: auto 0px;
	padding: 0;
	width: auto;
}
table {
	border-spacing: 0;
	border-collapse: collapse;
}
caption, th, td {
	text-align: left;
	font-weight: 400;
}
blockquote:before, blockquote:after, q:before, q:after {
	content: "";
}
blockquote, q {
	quotes: "" "";
}
form {
	margin-bottom: 0;
}
a:link, a:visited, a:active {
  color:#183A65 !important;
  border-bottom: 1px dotted transparent;
  
}
a:hover {
	color:#002642;
  border-color: #999; 
  text-decoration: none;
}
a img {
	border: 0;
}

li img{
float:none}
hr {
	color: #ccc;
	background: #ccc;
	border: 0;
}
fieldset {
	border: 0;
}

input[type="text"], input[type="password"], textarea{
	webkit-border-radius:6px;
  -moz-border-radius:6px;
  border-radius:6px;
  border:1px solid #ccc;
  cursor:text !important}
  
  textarea{
  resize: none;
  padding:4px;
  margin-bottom:8px}
  
input, button {
font-size: 0.9em;
  white-space: normal;
 	font-family: 'DroidSansRegular', Arial, sans-serif;
 	padding: 4px 4px;
	cursor:pointer;
	cursor:hand;
}
form[class="adminsearchform"] input{
padding:3px 5px}

input[type="button"], input[type="submit"], button{
webkit-border-radius:10px;
  -moz-border-radius:10px;
  border-radius:10px;
  border:1px solid #97b23c;
  background: transparent url([[pix:theme|b_btn]]) repeat-x left bottom;
  color:#183A65;
  margin: 2px 5px 
}

#id_cancel{
webkit-border-radius:10px;
  -moz-border-radius:10px;
  border-radius:10px;
  border:1px solid #e5005c !important;
  background: transparent url([[pix:theme|b_btn2]]) repeat-x left bottom;
  color:#fff !important;
}

div.singlebutton form input, div[class="navbutton"] input[type="submit"]{
background-color:#26A6DD !important;
background: transparent url([[pix:theme|btns_2]]) repeat-x left bottom;
color:#fff;
border-color:#26A6DD;
}

input[type="button"]:hover,
input[type="submit"]:hover,
button:hover,
div[class="navbutton"] input[type="submit"]:hover,
#id_cancel:hover{
background-position: left top;
}

p {
	margin: 0 0 1.5em;
	font-family: 'DroidSansRegular', Arial, sans-serif;
}
li p {
  margin: 0;
}
h1, h2, h3, h4, h5, h6 {
	font-weight: 700;
	margin: 0 0 1em;
	font-family: Arial, Helvetica, sans-serif;
}
h1, h2 {
	font-size: 19px;
}
h3, h4 {
	font-size: 18px;
}
h5, h6 {
	font-size: 16px;
}
ul {
	list-style-type: disc;
	margin: 0.5em 1.5em;
	font-family: 'DroidSansRegular', Arial, sans-serif;
}
/* General*/
.pagelayout-frontpage,.pagelayout-incourse,
.pagelayout-general, .pagelayout-course,
.pagelayout-coursecategory, .pagelayout-admin,
.pagelayout-mydashboard, .pagelayout-mypublic,
.pagelayout-login, .pagelayout-report {
  background:#666;
  border-top:0px solid #ccc;
}
#wrapper {
background:#fff url([[pix:theme|top_bkg]]) repeat-x;  
  
/*   webkit-border-radius:6px;
  -moz-border-radius:6px;
  border-radius:6px;
  -webkit-box-shadow:0px 0px 10px #000000;
  -moz-box-shadow:0px 0px 10px #000000;
  box-shadow:0px 0px 10px #000000; */
  
}
.pagelayout-frontpage #page-content #region-main-box #region-post-box {
  padding-top:0;
}
#page {
  width: 100%;
  overflow: hidden;
  min-width:960px
  }
#page-content {
  clear: both;
  float: left;
  overflow: hidden;
  position: relative;
  width: 100%;
  background:#fff url([[pix:theme|content_bg]]) repeat-x 0 0;
}
#page-content #region-main-box {
  float: none;
  margin-left: 0;
  overflow: hidden;
  width: 100%;
}
#page-content #region-main-box #region-post-box {
  float: none;
  margin-left: 0;
  width: 100%;
}
#page-content #region-main-box #region-post-box #region-main-wrap {
  float: none;
  width: auto;
}
#page-content #region-main-box #region-post-box #region-main-wrap #region-main {
  overflow: hidden;
  position: relative;
  margin-left: 0;
  left: 0;
}

#region-main a{
color:#183A65;}

#page-content #region-main-box #region-post-box #region-pre {
  float: left;
  left: 0;
  width: 250px;
}
#page-content #region-main-box #region-post-box #region-post {
  float: right;
  position: relative;
  left: 0;
  width: 250px;
}
#page-content #region-main-box #region-post-box #region-main-wrap #region-main .region-content {
  overflow: hidden;
  padding: 10px;
}
#page-content #region-main-box #region-post-box #region-pre .region-content,
#page-content #region-main-box #region-post-box #region-post .region-content {
  overflow: hidden;
  padding: 10px;
}
.has_dock.side-post-only .page-middle #region-main-box #region-post-box #region-main-wrap #region-main {
  margin-left: 200px;
}
a.dimmed, a.dimmed_text, .dimmed_text, .dimmed_text a, .dimmed_category, .dimmed_category a {
  color: #333;
}
/* Header*/
#page-header {
  background: transparent url([[pix:theme|top_img]]) no-repeat top right;
  float: right;
  width: 100%;
/*   border: 0px solid #ccc;
 */  padding:0;
  position:relative;
  overflow:hidden;
/*   -webkit-border-radius:6px;
  -moz-border-radius:6px;
  border-radius:6px; */
  
}
.headermain {
  text-indent: -9999px;
  display: block;
  width: 222px;
  height: 89px;
  margin: 12px 40px 20px 40px;
  background:url([[pix:theme|logo]]) no-repeat 0 0;
  float: left;
}
.headermenu {
	float: right;
	text-align: right;
	margin-top:25px;
	padding: 20px 10px 0 0;
	display:block;
	}
div.headermenu div.logininfo {
	margin-bottom: 10px;
	font-size: 11px;
	background: transparent url([[pix:theme|logininfo_bg]]) no-repeat 0 0;
	padding: 2px 0 2px 25px;
	color: #fff;
}
div.headermenu div.logininfo a, div.footermenu div.logininfo a {
	border-bottom: 0px dashed #ccc;
	color: #CBDE91 !important;
	font-weight: 700;
}

.top-image{
	height:122px;
	width:608px;
	background: transparent url([[pix:theme|top_img]]) no-repeat right;
	float: right;
}


/* Navbar */
.navbar {
  background: transparent url([[pix:theme|nav_bkg]]) repeat-x 0 0;
  padding:2px 0 2px 12px;
  line-height:30px;
  border: 1px solid #339933 !important;
  border-right:none !important;
}
.navbar .navbutton {
	margin-top: 0px;
	float: right;
}
.navbar .navbutton div, .navbar .navbutton form {
	display: inline;
	margin: 0;
	padding: 0;
}
.navbar .menu {
	float: right;
}
.navbar .breadcrumb {
	padding-left: 0.3em;
	float: left;
	margin: 0px 0;
}
.breadcrumb ul {
	text-indent: 0;
	list-style: none;
	margin: 0;
	padding: 0;
	font-family: 'DroidSansRegular', Arial, sans-serif;
}
.breadcrumb a {
color: #444445 !important;
font-weight:700;

}

.breadcrumb li {
	display: inline;
	color: #444445 !important;
	font-weight:700;
	
}
.breadcrumb li a, .breadcrumb li a:link, .breadcrumb li a:visited, .breadcrumb li a:hover {
	color: #444445 !important;
	font-weight:700;
	text-decoration:none !important;
	
}
.navigation li, .navigation div, .navigation button, .navigation form {
	display: inline;
}
.breadcrumb .sep, #admin-report-unittest-index .sep {
	color: #444445;
	font-weight:700;
}
/* Blocks */
.block {
  border: 0px solid #D6E1E5;
  -webkit-border-radius:12px 12px 0 0;
  -moz-border-radius:12px 12px 0 0;
  border-radius:12px 12px 0 0;
  background: #EBEBEB url([[pix:theme|shadow]]) no-repeat center bottom;
  padding-bottom:8px;
  overflow:hidden;
}

.block_navigation, .block_course_overview{
background-color:transparent !important;
background-image:none
}

.block_navigation .content ul li ul li{
border-bottom:1px solid #ccc}

.block_navigation .content ul li ul li ul li{
border:none}

.block_navigation .block_tree .active_tree_node, .active_tree_node {
    font-weight: normal !important;
	color:#183A65 !important
}

p  > a{ color:#183A65 !important}
.block .header, .questionbankwindow div.header {
