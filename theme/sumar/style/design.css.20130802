/*Calendar*/

.block_calendar_month .minicalendar, .minicalendar, .calendartable {
  border-collapse: separate;
  border-spacing: 1px;
  color: #666;
}
.block_calendar_month .minicalendar td, .minicalendar td, .calendartable td {
  border: 1px solid #C1C1C1;
  padding: 1px;
}
.block_calendar_month .minicalendar td.weekend, .minicalendar td.weekend, .calendartable td.weekend {
  color: #333 !important;
}
.block_calendar_month .minicalendar td.today, .minicalendar td.today {
	background-color: #193966 !important;
	border-color: #C1C1C1 !important;
	color: #fff !important;
}
